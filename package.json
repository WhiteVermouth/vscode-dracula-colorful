{
  "name": "vscode-dracula-colorful",
  "displayName": "Dracula Colorful",
  "description": "A vibrant variant of Dracula theme with bold accents, colorful brackets, and balanced contrast for comfortable long coding sessions",
  "version": "1.1.1",
  "publisher": "nszihan",
  "author": {
    "name": "Zihan Ma"
  },
  "sponsor": {
    "url": "https://buymeacoffee.com/nszihan"
  },
  "license": "Apache-2.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dracula",
    "dark theme",
    "colorful",
    "theme",
    "color-theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WhiteVermouth/vscode-dracula-colorful"
  },
  "bugs": {
    "url": "https://github.com/WhiteVermouth/vscode-dracula-colorful/issues"
  },
  "homepage": "https://github.com/WhiteVermouth/vscode-dracula-colorful#readme",
  "devDependencies": {
    "@vscode/vsce": "^3.2.1",
    "ovsx": "^0.10.1"
  },
  "scripts": {
    "package": "vsce package",
    "vscode:publish": "vsce publish",
    "vscode:publish:patch": "vsce publish patch",
    "vscode:publish:minor": "vsce publish minor",
    "vscode:publish:major": "vsce publish major",
    "ovsx:publish": "ovsx publish",
    "ovsx:publish:patch": "ovsx publish --patch",
    "ovsx:publish:minor": "ovsx publish --minor",
    "ovsx:publish:major": "ovsx publish --major",
    "publish": "vsce publish && ovsx publish",
    "publish:patch": "vsce publish patch && ovsx publish --patch",
    "publish:minor": "vsce publish minor && ovsx publish --minor",
    "publish:major": "vsce publish major && ovsx publish --major"
  },
  "contributes": {
    "themes": [
      {
        "label": "Dracula Colorful",
        "uiTheme": "vs-dark",
        "path": "./themes/Dracula Colorful-color-theme.json"
      }
    ],
    "configurationDefaults": {
      "[Dracula Colorful]": {
        "editor.semanticHighlighting.enabled": true
      }
    }
  }
}

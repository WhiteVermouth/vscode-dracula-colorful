  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#98afff"
      }
    },
    {
      "name": "Doc comments and tags",
      "scope": [
        "comment.block.documentation",
        "comment.block.documentation keyword",
        "comment.block.documentation storage",
        "comment.block.documentation punctuation.definition",
        "comment.block.documentation punctuation.definition.tag",
        "comment.block.documentation punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#98afff"
      }
    },
    {
      "name": "Doc tags and values",
      "scope": [
        "comment.block.documentation entity.name",
        "comment.block.documentation storage.type",
        "comment.block.documentation string"
      ],
      "settings": {
        "foreground": "#ffb86c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Todo in comments",
      "scope": [
        "comment.keyword.todo",
        "comment.keyword.fixme",
        "comment.keyword.note",
        "comment.keyword.xxx"
      ],
      "settings": {
        "foreground": "#e998ff"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "name": "String escapes and regex",
      "scope": [
        "constant.character.escape",
        "string.regexp",
        "constant.other.character-class",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Numbers and constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Keywords and storage",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Operators and punctuation",
      "scope": [
        "keyword.operator",
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.braces.begin",
        "punctuation.section.braces.end",
        "punctuation.section.block.begin",
        "punctuation.section.block.end",
        "punctuation.section.curly",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#737fff"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.section.brackets",
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.section.square.begin",
        "punctuation.section.square.end",
        "punctuation.section.square",
        "meta.brace.square"
      ],
      "settings": {
        "foreground": "#36ffac"
      }
    },
    {
      "name": "Parentheses",
      "scope": [
        "punctuation.section.parens",
        "punctuation.section.parens.begin",
        "punctuation.section.parens.end",
        "punctuation.section.group.begin",
        "punctuation.section.group.end",
        "punctuation.section.parameters.begin",
        "punctuation.section.parameters.end",
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#fff906"
      }
    },
    {
      "name": "Functions and methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Types and classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Annotations",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.type.annotation"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Variables and parameters",
      "scope": [
        "variable",
        "variable.other",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#b9bcd1"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter.function",
        "variable.parameter.method",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#b9bcd1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Properties and members",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "variable.other.readwrite",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "variable.other.constant",
        "entity.name.constant",
        "support.constant.enum"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "HTML and XML entities",
      "scope": [
        "constant.character.entity",
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "name": "JSON keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "CSS property names",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.sass",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.stylus"
      ],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "CSS classes and IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Markup headings",
      "scope": [
        "markup.heading",
        "markup.heading.entity.name"
      ],
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup bold/italic",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#ff79c6",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup inserted/deleted/changed",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Markup deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "name": "Markup changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Markdown links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#8be9fd",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff5555"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#bd93f9",
    "type": "#8be9fd",
    "typeParameter": "#bd93f9",
    "class": "#8be9fd",
    "enum": "#8be9fd",
    "interface": "#8be9fd",
    "struct": "#8be9fd",
    "function": "#50fa7b",
    "method": "#50fa7b",
    "property": "#ffb86c",
    "variable": "#b9bcd1",
    "parameter": {
      "foreground": "#b9bcd1",
      "fontStyle": "italic"
    },
    "enumMember": "#bd93f9",
    "keyword": "#ff79c6",
    "string": "#f1fa8c",
    "number": "#bd93f9",
    "comment": {
      "foreground": "#98afff",
      "fontStyle": "italic"
    }
  }
}
